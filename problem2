print("\t\tAttendance shortage Calculator")
Tot=float(input("Enter total no.of classes"))
att=float(input("Enter no.of classes attended"))
percent=(att/Tot)*100
print("ATTENDANCE PERCENTAGE:",percent)
if(percent>=75):
    print("Status:Eligible")
else:
    print("Status:Not Eligible")
    remain=((75-percent)*Tot)/100
    print("additional classes required",remain)
